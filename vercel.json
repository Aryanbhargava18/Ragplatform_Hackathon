{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@streamlit/vercel-builder"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PORT": "8501"
    }
} 